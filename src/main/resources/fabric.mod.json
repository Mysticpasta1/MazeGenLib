{
  "schemaVersion": 1,
  "id": "mazegenlib",
  "version": "${version}",

  "name": "MazeGenLib",
  "description": "a library mod that generates mazes in other mods",
  "authors": [
    "Mysticpasta1, PseudoDistant"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/Mysticpasta1/MazeGenLib"
  },

  "license": "MIT",
  "icon": "assets/mazegenlib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.mystic.mazegenlib.MazeGenLib"
    ]
  },
  "mixins": [
    "mazegenlib.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
